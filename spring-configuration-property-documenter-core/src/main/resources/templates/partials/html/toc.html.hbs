{{#templateCustomization}}
{{#if tableOfContentsEnabled}}
<div class="center">
    <button type="button" class="collapsible">{{#if tocTitle}}{{tocTitle}}{{else}}{{i18n "template.toc"}}{{/if}}</button>
    <div id="toc-content">

        {{#if subTemplateDataList}}
            <ul>
                {{#each subTemplateDataList}}
                    <li><a href="#{{moduleName}}">{{moduleName}}</a>
                        <ul>
                            {{#each propertyGroups}}
                                <li><a href="#{{groupName}}">{{groupName}} - <code>{{type}}</code></a></li>
                            {{/each}}
                        </ul>
                    </li>
                {{/each}}
            </ul>
        {{else}}
            <ul>
                {{#each propertyGroups}}
                    <li><a href="#{{groupName}}">{{groupName}} - <code>{{type}}</code></a></li>
                {{/each}}
            </ul>
        {{/if}}
    </div>
</div>
{{/if}}
{{/templateCustomization}}
