        <module>
            {{#if moduleName}}<moduleName> {{moduleName}}</moduleName>{{/if}}
            {{#if moduleDescription}}<moduleDescription> {{moduleDescription}}</moduleDescription>{{/if}}
            <propertyGroups>{{#each propertyGroups}}
                    <propertyGroup>
                        <groupName>{{groupName}}</groupName>
                        {{#if type}}<type>{{type}}</type>{{/if}}
                        <properties>{{#each properties}}
                            <property>
                                <key>{{key}}</key>
                                {{#if type}}<type>{{type}}</type>{{/if}}
                                <description>{{description}}</description>
                                <defaultValue>{{defaultValue}}</defaultValue>
                                <propertyDeprecation>{{propertyDeprecation}}</propertyDeprecation>
                                {{#is_included "envFormat"}}<environmentVariable>{{as_env fqName}}</environmentVariable>{{/is_included}}
                            </property>{{/each}}
                        </properties>
                    </propertyGroup>{{/each}}
            </propertyGroups>
        </module>
